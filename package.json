        "lodash": "^4.17.4",
        "popper.js": "^1.12.5",
        "vue": "^2.4.4",
        "vue-router": "^2.7.0"
    },
    "dependencies": {
        "laravel-echo": "^1.3.2",
        "moment": "^2.18.1",
        "vee-validate": "^2.0.0-beta.17",
